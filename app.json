{
  "expo": {
    "name": "تدبر",
    "slug": "tadabur",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-logo.jpeg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-logo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abdalghanidakkak.tadabur",
      "permissions": [
        "FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-logo.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "932565a2-7159-4f18-867e-d281fbf85483"
      }
    }
  }
}
